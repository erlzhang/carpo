- if notice
  .alert.alert-info.alert-float
    = notice
    %button.close{"data-dismiss" => "alert", "aria-label" => "Close"}
      %span.icon-close{"aria-hidden" => "true"}
- if alert
  .alert.alert-warning.alert-float
    %span.icon-warning.mr-2
    = alert
    %button.close{"data-dismiss" => "alert", "aria-label" => "Close"}
      %span.icon-close{"aria-hidden" => "true"}
